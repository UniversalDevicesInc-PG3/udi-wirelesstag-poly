<nodeDefs>
  <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
  <!-- controller -->
  
  <nodeDef id="wstCntl" nls="cntl">
    <editors />
    <sts>
      <st id="ST" editor="BOOL" />
      <!-- Version Major -->
      <st id="GV1" editor="FLOAT" />
      <!-- Version Minor -->
      <st id="GV2" editor="FLOAT" />
      <!-- wirelesstag.net communicating -->
      <st id="GV3" editor="BOOL" />
    </sts>
    <cmds>
      <sends />
      <accepts>
        <cmd id="QUERY" />
        <cmd id="DISCOVER" />
        <cmd id="INSTALL_PROFILE" />
      </accepts>
    </cmds>
  </nodeDef>

  
  <!-- Tag Manager -->
  <nodeDef id="wstTagManager" nls="tagmgr">
    <editors />
    <sts>
      <st id="ST" editor="bool" />
    </sts>
    <cmds>
      <sends />
      <accepts>
        <cmd id="DON" />
        <cmd id="DOF" />
      </accepts>
    </cmds>
  </nodeDef>

  
  <!-- Tag 12 -->
  <nodeDef id="wst12" nls="tag12">
    <editors />
    <sts>
      <st id="ST" editor="BOOL" />
      <st id="GV1" editor="B_ONOFF" />
      <st id="CLITEMP" editor="I_TEMP_F" />
      <st id="BATLVL" editor="BATLVL" />
      <st id="CV" editor="FLOAT" />
      <st id="GV2" editor="I_MOTION" />
      <st id="GV3" editor="FLOAT" />
      <st id="GV4" editor="FLOAT" />
      <st id="GV5" editor="FLOAT" />
      <st id="GV6" editor="FLOAT" />
    </sts>
    <cmds>
      <sends />
      <accepts>
        <cmd id="DON" />
        <cmd id="DOF" />
      </accepts>
    </cmds>
  </nodeDef>

</nodeDefs>
