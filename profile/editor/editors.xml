<editors>
    <!-- Boolean -->
    <editor id="BOOL">
        <range uom="2" subset="0,1" />
    </editor>
    <editor id="FLOAT">
       <range uom="56" min="0.1" max="999.999"/>
    </editor>

    <editor id="I_INTEGER">
      <range uom="56" min="0" max="4294967295" prec="0" />
    </editor>

    <editor id="I_TAGID">
      <range uom="56" min="0" max="92" prec="0" />
    </editor>

    <editor id="I_DM">
        <range uom="25" subset="0,10,20,30,40,50" nls="CDM" />
    </editor>

    <editor id="B_ONOFF">
        <range uom="2" subset="0,1" nls="ONOFF" />
    </editor>

    <!-- Temperature -->
   <editor id="I_TEMP">
      <range uom="14" min="-50" max="200" prec="2" />
   </editor>
   <editor id="I_TEMP_C">
      <range uom="4" min="-50" max="75"  prec="2" />
   </editor>
   <editor id="I_TEMP_F">
      <range uom="17" min="-50" max="150" prec="2" />
   </editor>

   <editor id="I_HUMIDITY">
     <range uom="51" min="0" max="100" prec="0" />
   </editor>

   <editor id="I_LUMIN">
     <range uom="36" min="0" max="100" prec="0" />
   </editor>

   <!-- Voltage -->
   <editor id="I_VOLT">
      <range uom="72" min="0" max="10" prec="3" />
   </editor>
   
    <!-- Motion -->
   <editor id="I_MOTION">
      <range uom="25" subset="0,1,2,3" nls="MOT4" />
   </editor>
   
   <!-- Event State-->
   <editor id="I_EVST">
      <range uom="25" subset="0,1,2,3,4,5,6" nls="EVST" />
   </editor>

</editors>
